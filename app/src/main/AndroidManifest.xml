<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gradiuation.stopwatch" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Other_Medicaton"
            android:label="@string/title_activity_other__medication"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Events"
            android:label="@string/title_activity_events"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".rhythm"
            android:label="@string/title_activity_rhythm"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".Rosc"
            android:label="@string/title_activity_rosc"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Setting"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_setting" >
        </activity>
    </application>

</manifest>
